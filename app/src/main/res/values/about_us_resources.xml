<resources>


    <!--Contact info arrays-->

    <!--In this array you will put the icons for contact information,
    for example xda icon for your xda thread, site icon for your site, facebook icon and so on-->
    <array name="about_contact_us_drawables">
        <item>@drawable/xda_logo</item>
    </array>

    <!--In this array you will put the titles for your contact info items. Keep the order consistent between all 3 arrays-->
    <string-array name="about_contact_us_text">
        <item>XDA</item>
    </string-array>

    <!--In this array you will put the titles for the URL strings that will open once a user clicked on the item-->
    <string-array name="about_contact_us_links" translatable="false">
        <item>http://forum.xda-developers.com/</item>
    </string-array>



    <!--Team info arrays-->
    <array name="about_team_drawables">
        <item>@drawable/lego_starwars_saga</item>
    </array>
    <string-array name="about_team_names">
        <item>Niaboc79 - Developer</item>
    </string-array>
    <string-array name="about_team_links" translatable="false">
        <item>http://forum.xda-developers.com/member.php?u=599722</item>
    </string-array>



    <!--Credits and thanks info arrays-->
    <array name="about_credits_drawables">
        <item>@drawable/little_alchemy</item>
    </array>
    <string-array name="about_credits_names">
        <item>XDA | serajr | auras76</item>
    </string-array>
    <string-array name="about_credits_links" translatable="false">
        <item>http://forum.xda-developers.com/</item>
    </string-array>
</resources>